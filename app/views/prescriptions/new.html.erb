<% content_for :page_title do %>
  <h2>New Medication Change for <%= @patient.ident %></h2>
<% end %>

<%= form_for([@patient, @prescription], html: { class: 'form-horizontal' }) do |f| %>
  <%= errors_in(@prescription) %>
  <%= render partial: 'prescriptions/form', locals: { f: f }  %>
  <hr/>
  <div class="control-group">
    <div class="controls">
      <%= f.submit %>
      <%= link_to 'Back', patient_prescriptions_path(@patient) %>
    </div>
  </div>
<% end %>

